{
    "presets": ["@babel/preset-env"],
    "plugins": [
        "@babel/plugin-proposal-export-default-from",
        [
            "babel-plugin-root-import",
            {
                "paths": [
                    {
                        "rootPathPrefix": "~/",
                        "rootPathSuffix": "src"
                    },
                    {
                        "rootPathPrefix": "?/",
                        "rootPathSuffix": "test"
                    }
                ]
            }
        ]
    ]
}
